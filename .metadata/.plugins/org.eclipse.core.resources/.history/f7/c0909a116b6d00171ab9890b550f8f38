
public class MyThread {

	public static void main(String[] args) {
		
		Thread t1= new Thread (){
			public void run(){
				for(int i=0; i<20;i++){
					System.out.println("Printing..."+i*5);
					try {
						sleep(1000000);
					} catch (InterruptedException e) {
						// TODO Auto-generated catch block
						e.printStackTrace();
					}
				}
			}
		};
		
		t1.start();

	}

}

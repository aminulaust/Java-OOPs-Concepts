
public class MyLoops {
	
	public static void main(String[] args){
		int checker=0;
		
		do{
			System.out.println("Everything is ok " +checker);
			checker++;
			
		}
		while(checker<20);
		
		
		/*
		while(checker<20){
			System.out.println("Everything is ok " +checker);
			checker++;
		}
		*/
	}

}
